<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vistas.DomicilioActivoActivity">

    <ImageButton
        android:id="@+id/btnDoAcLogout"
        android:layout_width="79dp"
        android:layout_height="76dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_logout_peq" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="282dp"
        android:layout_height="76dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnDoAcLogout"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/logo_uno" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="341dp"
        android:layout_height="39dp"
        android:layout_marginTop="24dp"
        android:text="Domicilio en Curso"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.342"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider6"
        android:layout_width="363dp"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/txtDoAcDos"
        android:layout_width="331dp"
        android:layout_height="30dp"
        android:layout_marginTop="3dp"
        android:text="Origen : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcUno" />

    <TextView
        android:id="@+id/txtDoAcCuatro"
        android:layout_width="334dp"
        android:layout_height="29dp"
        android:layout_marginTop="6dp"
        android:text="Cliente : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcTres" />

    <TextView
        android:id="@+id/txtDoAcTres"
        android:layout_width="329dp"
        android:layout_height="28dp"
        android:layout_marginTop="3dp"
        android:text="Destino : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcDos" />

    <TextView
        android:id="@+id/txtDoAcUno"
        android:layout_width="328dp"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        android:text="Descripcion : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider6" />

    <TextView
        android:id="@+id/txtDoAcSeis"
        android:layout_width="333dp"
        android:layout_height="23dp"

        android:layout_marginTop="6dp"
        android:text="Hora : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcCinco" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:text="Evidencias Origen"
        app:layout_constraintEnd_toStartOf="@+id/btnDoAcAdduno"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcSeis" />

    <ImageButton
        android:id="@+id/btnDoAcAddNote"
        android:layout_width="59dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/btnDoAcAdddos"
        app:srcCompat="@drawable/img_add_note" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="135dp"
        android:layout_height="22dp"
        android:layout_marginTop="8dp"
        android:text="Evidencias Destino"
        app:layout_constraintEnd_toStartOf="@+id/btnDoAcAdddos"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDoAcAdduno" />

    <ImageButton
        android:id="@+id/btnDoAcAdduno"
        android:layout_width="60dp"
        android:layout_height="57dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcSeis"
        app:srcCompat="@drawable/img_anadir" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="134dp"
        android:layout_height="20dp"
        android:layout_marginTop="16dp"
        android:text="AÃ±adir Nota"
        app:layout_constraintEnd_toStartOf="@+id/btnDoAcAddNote"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDoAcAdddos" />

    <ImageButton
        android:id="@+id/btnDoAcAdddos"
        android:layout_width="59dp"
        android:layout_height="54dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/btnDoAcAdduno"
        app:srcCompat="@drawable/img_anadir" />

    <TextView
        android:id="@+id/txtDoAcCinco"
        android:layout_width="336dp"
        android:layout_height="24dp"
        android:layout_marginTop="6dp"
        android:text="Fecha  : "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDoAcCuatro" />

</androidx.constraintlayout.widget.ConstraintLayout>